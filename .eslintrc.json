{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 13,
		"sourceType": "module"
	},
	"plugins": ["prettier", "only-warn"],
	"extends": ["prettier"],
	"rules": {
		"prettier/prettier": ["error", { "singleQuote": true, "printWidth": 120, "semi": false }],
		"no-unused-vars": "off"
	}
}
